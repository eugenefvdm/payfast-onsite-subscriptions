{
    "name": "fintechsystems/payfast-onsite-subscriptions",
    "description": "A Laravel service provider for subscription billing",
    "keywords": [        
        "laravel",
        "payfast",
        "subscription-billing"
    ],
    "homepage": "https://github.com/fintechsystems/payfast-onsite-subscriptions",
    "authors": [
        {
            "name": "Eugene van der Merwe",
            "email": "eugenevdm@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FintechSystems\\PayFast\\": "src/"            
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "FintechSystems\\PayFast\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "symfony/dotenv": "^5.3",
        "illuminate/contracts": "^8.37",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {  
        "nunomaduro/collision": "^5.3",      
        "orchestra/testbench": "^6.20",           
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FintechSystems\\PayFast\\PayFastServiceProvider"
            ],
            "aliases": {
                "payfast": "FintechSystems\\PayFast\\Facades\\PayFast"
            }
        }
    }
}
